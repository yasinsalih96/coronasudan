<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
	<!-- insert your AIML categories here -->
	<category>
	    <pattern>MY NAME IS *</pattern>
	    <template>
	        Hello <set name="username"> <star/>!</set> glad to see you.
	    </template>
    </category>
    
    <category>
	    <pattern>WHAT IS MY NAME</pattern>
	    <template>
	        Your name is <get name="username"> <star/>!</get>
	    </template>
    </category>
    
</aiml>